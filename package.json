{
  "name": "@golemio/output-gateway",
  "version": "2.0.0",
  "description": "Output gateway for Golemio data platform",
  "main": "index.js",
  "scripts": {
    "dev-start": "ts-node-dev -r dotenv/config ./src/index.ts",
    "dev-start-debug": "ts-node-dev --inspect-brk=9229 -r dotenv/config ./src/index.ts",
    "start": "node -r dotenv/config dist/index.js",
    "debug": "node --inspect-brk=9229 -r dotenv/config dist/index.js",
    "build": "tsc",
    "build-watch": "tsc --watch --preserveWatchOutput",
    "test": "mocha -b --check-leaks --timeout 60000 -r ts-node/register -r dotenv/config --recursive test/helpers/*.test.ts test/*.test.ts test/models/*.test.ts test/routes/*.test.ts --exit",
    "test-debug": "mocha --inspect-brk=9229 -b --check-leaks --timeout 60000 -r ts-node/register -r dotenv/config --recursive test/helpers/*.test.ts test/*.test.ts test/models/*.test.ts test/routes/*.test.ts --exit",
    "code-coverage": "nyc mocha -b --check-leaks --timeout 60000 -r ts-node/register -r source-map-support/register -r dotenv/config --recursive  test/helpers/*.test.ts test/*.test.ts test/models/*.test.ts test/routes/*.test.ts --exit",
    "generate-docs": "npx typedoc --readme \"./README.MD\" --name \"Golemio Output Gateway\" --mode file --out docs/typedoc src",
    "dredd": "dredd",
    "tslint": "tslint -p ."
  },
  "author": "Operator ICT, a.s.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/operator-ict/golemio/code/output-gateway"
  },
  "dependencies": {
    "@golemio/errors": "^0.0.6",
    "@golemio/schema-definitions": "1.0.1-dev.121231506",
    "@golemio/utils": "^0.0.3",
    "apicache": "^1.4.0",
    "debug": "^4.1.0",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "geojson-tools": "^0.2.2",
    "moment": "^2.24.0",
    "mongoose": "^5.3.4",
    "morgan": "^1.9.1",
    "object-hash": "^1.3.1",
    "pg": "^7.7.1",
    "redis": "^2.8.0",
    "sequelize": "4.41.2",
    "sequelize-typescript": "0.6.11",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "@types/apicache": "^1.2.0",
    "@types/chai": "^4.1.7",
    "@types/chai-as-promised": "^7.1.0",
    "@types/debug": "^4.1.0",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.1.12",
    "@types/mongoose": "^5.2.19",
    "@types/morgan": "^1.7.35",
    "@types/node": "^10.12.7",
    "@types/object-hash": "^1.3.0",
    "@types/redis": "^2.8.13",
    "@types/sequelize": "4.28.8",
    "@types/sinon": "^7.0.13",
    "@types/supertest": "^2.0.7",
    "@types/validator": "10.11.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dredd": "^11.2.9",
    "husky": "^3.0.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "sinon": "^7.2.3",
    "supertest": "^3.3.0",
    "ts-node": "^7.0.1",
    "ts-node-dev": "^1.0.0-pre.40",
    "tslint": "^5.11.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.1.3"
  },
  "nyc": {
    "check-coverage": true,
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist",
      "**/*.test.ts",
      "docs"
    ],
    "all": true,
    "lines": 0,
    "statements": 0,
    "functions": 0,
    "branches": 0
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tslint"
    }
  }
}
