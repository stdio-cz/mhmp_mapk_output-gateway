{
    "name": "@golemio/output-gateway",
    "version": "3.1.0",
    "description": "Output gateway for Golemio data platform",
    "main": "index.js",
    "scripts": {
        "dev-start": "ts-node-dev -r dotenv/config ./src/index.ts",
        "dev-start-debug": "ts-node-dev --inspect-brk=9229 -r dotenv/config ./src/index.ts",
        "start": "node -r dotenv/config dist/index.js",
        "debug": "node --inspect-brk=9229 -r dotenv/config dist/index.js",
        "build": "tsc",
        "build-minimal": "tsc --sourceMap false --declaration false",
        "build-watch": "tsc --watch --preserveWatchOutput",
        "build-apidocs": "ts-node -r dotenv/config ./src/doc-generator/index.ts && openapi-merge-cli --config ./src/doc-generator/generated-merge-configs/openapi-merge-config.json && openapi-merge-cli --config ./src/doc-generator/generated-merge-configs/public-openapi-merge-config.json",
        "test": "mocha --exit -b --check-leaks --timeout 60000 -r ts-node/register --file 'test/setup.ts' -r dotenv/config --recursive 'test/**/*.test.ts' --reporter mocha-junit-reporter --reporter-options mochaFile=junit.xml",
        "test-debug": "run-s 'test --inspect-brk=9230'",
        "code-coverage": "nyc run-s 'test -r source-map-support/register'",
        "generate-docs": "typedoc --out docs/typedoc src",
        "dredd": "dredd",
        "lint": "eslint \"src/**/*.ts\""
    },
    "author": "Operator ICT, a.s.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/operator-ict/golemio/code/output-gateway"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0",
        "yarn": "Use npm!"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@golemio/cli": "1.3.3",
        "@golemio/eslint-config": "1.1.0",
        "@types/chai": "^4.1.7",
        "@types/chai-as-promised": "^7.1.0",
        "@types/compression": "^1.7.1",
        "@types/mocha": "^8.2.1",
        "@types/node": "^16.11.35",
        "@types/sinon": "^7.0.13",
        "@types/supertest": "^2.0.7",
        "@types/swagger-ui-express": "^4.1.3",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "dredd": "^13.0.2",
        "eslint": "^8.1.1",
        "husky": "^3.0.0",
        "mocha": "^8.3.1",
        "mocha-junit-reporter": "^2.0.2",
        "npm-run-all": "^4.1.5",
        "nyc": "^15.1.0",
        "openapi-merge-cli": "^1.3.1",
        "prettier": "^2.5.1",
        "pretty-quick": "^3.1.0",
        "sinon": "^7.2.3",
        "supertest": "^3.3.0",
        "ts-node": "^10.7.0",
        "ts-node-dev": "^2.0.0",
        "typedoc": "^0.22.18",
        "typescript": "4.7.2"
    },
    "dependencies": {
        "@golemio/air-quality-stations": "1.1.0",
        "@golemio/bicycle-counters": "1.1.0",
        "@golemio/bicycle-parkings": "1.1.0",
        "@golemio/city-districts": "1.1.0",
        "@golemio/core": "1.4.0",
        "@golemio/exporting-module": "1.1.0",
        "@golemio/fcd": "1.1.0",
        "@golemio/flow": "1.2.0",
        "@golemio/gardens": "1.2.0",
        "@golemio/medical-institutions": "1.1.0",
        "@golemio/meteosensors": "1.1.0",
        "@golemio/municipal-authorities": "1.1.0",
        "@golemio/municipal-libraries": "1.2.0",
        "@golemio/municipal-police-stations": "1.2.0",
        "@golemio/ndic": "1.1.0",
        "@golemio/parking-zones": "1.1.0",
        "@golemio/parkings": "1.3.0",
        "@golemio/playgrounds": "1.1.0",
        "@golemio/public-toilets": "1.1.0",
        "@golemio/rush-hour-aggregation": "1.1.0",
        "@golemio/shared-bikes": "2.1.0",
        "@golemio/shared-cars": "1.1.0",
        "@golemio/sorted-waste-stations": "1.2.0",
        "@golemio/traffic-cameras": "1.1.0",
        "@golemio/waste-collection": "0.1.0",
        "@golemio/waste-collection-yards": "1.2.0",
        "compression": "^1.7.4",
        "dotenv": "^6.1.0",
        "fast-glob": "^3.2.11",
        "swagger-ui-express": "^4.3.0"
    },
    "overrides": {
        "qs": "^6.7.3"
    }
}
