{
    "name": "@golemio/output-gateway",
    "version": "2.4.1",
    "description": "Output gateway for Golemio data platform",
    "main": "index.js",
    "scripts": {
        "dev-start": "ts-node-dev -r dotenv/config ./src/index.ts",
        "dev-start-debug": "ts-node-dev --inspect-brk=9229 -r dotenv/config ./src/index.ts",
        "start": "node -r dotenv/config dist/index.js",
        "debug": "node --inspect-brk=9229 -r dotenv/config dist/index.js",
        "build": "tsc",
        "build-minimal": "tsc --sourceMap false --declaration false",
        "build-watch": "tsc --watch --preserveWatchOutput",
        "test": "mocha -b --check-leaks --timeout 60000 -r ts-node/register -r dotenv/config --recursive test/**/*.test.ts --exit",
        "test-debug": "mocha --inspect-brk=9229 -b --check-leaks --timeout 60000 -r ts-node/register -r dotenv/config --recursive test/**/*.test.ts --exit",
        "code-coverage": "nyc mocha -b --check-leaks --timeout 60000 -r ts-node/register -r source-map-support/register -r dotenv/config --recursive test/**/*.test.ts --exit",
        "generate-docs": "npx typedoc --readme \"./README.MD\" --name \"Golemio Output Gateway\" --mode file --out docs/typedoc src",
        "dredd": "dredd",
        "lint": "eslint src/**/*.ts"
    },
    "author": "Operator ICT, a.s.",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://gitlab.com/operator-ict/golemio/code/output-gateway"
    },
    "devDependencies": {
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@golemio/eslint-config": "^1.0.2",
        "@types/chai": "^4.1.7",
        "@types/chai-as-promised": "^7.1.0",
        "@types/mocha": "^5.2.5",
        "@types/node": "^10.12.7",
        "@types/sinon": "^7.0.13",
        "@types/supertest": "^2.0.7",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "dredd": "^11.2.9",
        "eslint": "^7.18.0",
        "geojson-tools": "^0.2.2",
        "husky": "^3.0.0",
        "mocha": "^5.2.0",
        "npm-run-all": "^4.1.5",
        "nyc": "^13.1.0",
        "prettier": "^2.2.1",
        "pretty-quick": "^3.1.0",
        "sinon": "^7.2.3",
        "supertest": "^3.3.0",
        "ts-node": "^7.0.1",
        "ts-node-dev": "^1.0.0-pre.40",
        "typedoc": "^0.20.28",
        "typescript": "^3.8.3"
    },
    "dependencies": {
