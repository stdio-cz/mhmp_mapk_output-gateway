        "@types/sinon": "^7.0.13",
        "@types/supertest": "^2.0.7",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "dredd": "^11.2.9",
        "eslint": "^7.18.0",
        "husky": "^3.0.0",
